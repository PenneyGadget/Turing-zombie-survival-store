<h1 class="center">Welcome back...</h1>

<div id="login-form" class="container">

  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email, class: "form-text" %>
    <%= f.text_field :email %>

    <%= f.label :password, class: "form-text" %>
    <%= f.password_field :password %>

    <%= f.submit "Login" %>
  <% end %>

</div>
